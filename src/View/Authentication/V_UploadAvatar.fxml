<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Authentication.C_UploadAvatar">
   <children>
      <Label layoutX="66.0" layoutY="35.0" text="Upload Profile Picture">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Circle fx:id="circle_Avatar" fill="WHITE" layoutX="129.0" layoutY="173.0" radius="80.0" stroke="#2196f3" strokeType="INSIDE" />
      <JFXButton fx:id="btn_Next" layoutX="153.0" layoutY="349.0" onAction="#btn_NextOnAction" prefHeight="28.0" prefWidth="86.0" style="-fx-background-color: #2196f3; -fx-text-fill: white;" text="Next" />
      <JFXButton fx:id="btn_Skip" layoutX="14.0" layoutY="351.0" onAction="#btn_SkipOnAction" prefHeight="25.0" prefWidth="51.0" text="Skip" />
      <Circle fill="DODGERBLUE" layoutX="177.0" layoutY="238.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
      <ImageView fx:id="img_AddImage" fitHeight="24.0" fitWidth="24.0" layoutX="165.0" layoutY="226.0" onMouseClicked="#img_AddImageOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/google_images_24px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
